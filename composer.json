{
    "name": "corephp/msgphp-user-bundle",
    "type": "symfony-bundle",
    "description": "Basic user management (the `User` domain)",
    "keywords": [
        "user",
        "security"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
     "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/corePHP/msgphp_user"
        },
        {
            "type": "vcs",
            "url": "https://github.com/corePHP/msgphp_domain"
        }
    ],
    "require": {
        "corephp/msgphp-user": "^1.0.6",
        "symfony/config": "^3.4 || ^4.4 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.4 || ^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-main"
        }
    },
    "autoload": {
        "psr-4": {
            "MsgPhp\\UserBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
